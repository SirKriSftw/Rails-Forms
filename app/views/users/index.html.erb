<h1>Users</h1>

<table>
    <thead>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td> <%= user.id %> </td>
                <td> <%= user.username %> </td>
                <td> <%= user.email %> </td>
            </tr>
        <% end %>
    </tbody>
</table>